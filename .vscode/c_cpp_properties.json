{
    "version": 4,
    "configurations": [
        {
            "name": "Linux Kernel Development",
            "intelliSenseMode": "linux-gcc-x64",
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu11",
            "cppStandard": "gnu++17",
            "includePath": [
                "${workspaceFolder}",
                "/usr/lib/gcc/x86_64-pc-linux-gnu/14.2.1/include",
                "/usr/local/include/",
                "/usr/lib/gcc/x86_64-pc-linux-gnu/14.2.1/include-fixed",
                "/usr/include",
                "/usr/lib/modules/6.13.5-arch1-1/build/include",
                "/usr/lib/modules/6.13.5-arch1-1/build/arch/x86/include/generated",
                "/usr/lib/modules/6.13.5-arch1-1/build/arch/x86/include"
            ],
            "defines": [
                "__KERNEL__",
                "MODULE"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/**",
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ]
}