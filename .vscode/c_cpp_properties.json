{
	"version": 4,
	"configurations": [
		{
			"name": "Linux Kernel Development",
			"intelliSenseMode": "linux-gcc-x64",
			"compilerPath": "/usr/bin/gcc",
			"compilerArgs": [
				"-ffreestanding",
				"-nostdinc",
				"-nostdlib"
			],
			"forcedInclude": [
				"/usr/src/linux-6.12.41-gentoo-dist/include/linux/compiler-version.h",
				"/usr/src/linux-6.12.41-gentoo-dist/include/linux/kconfig.h",
				"/usr/src/linux-6.12.41-gentoo-dist/include/linux/compiler_types.h"
			],
			"cStandard": "gnu11",
			"includePath": [
				"/usr/src/linux-6.12.41-gentoo-dist/arch/x86/include",
				"/usr/src/linux-6.12.41-gentoo-dist/arch/x86/include/generated",

				"/usr/src/linux-6.12.41-gentoo-dist/include",

				"/usr/src/linux-6.12.41-gentoo-dist/arch/x86/include/uapi",
				"/usr/src/linux-6.12.41-gentoo-dist/arch/x86/include/generated/uapi",

				"/usr/src/linux-6.12.41-gentoo-dist/include/uapi",
				"/usr/src/linux-6.12.41-gentoo-dist/include/generated/uapi"
			],
			"defines": [
				"__KERNEL__",
				"MODULE"
			],
			"browse": {
				"path": [
					"${workspaceFolder}/**",
					"${workspaceFolder}"
				],
				"limitSymbolsToIncludedHeaders": true,
				"databaseFilename": ""
			}
		}
	]
}
