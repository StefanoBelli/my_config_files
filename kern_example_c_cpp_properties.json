{
    "version": 4,
    "configurations": [
        {
            "name": "Linux Kernel Development",
            "intelliSenseMode": "linux-gcc-x64",
            "compilerPath": "/usr/bin/gcc",
			"compilerArgs": [
				"-ffreestanding",
				"-nostdinc",
				"-nostdlib"
			],
            "cStandard": "gnu11",
            "includePath": [
                "/usr/lib/modules/6.16.7-arch1-1/build/arch/x86/include",
				"/usr/lib/modules/6.16.7-arch1-1/build/arch/x86/include/generated",

                "/usr/lib/modules/6.16.7-arch1-1/build/include",

                "/usr/lib/modules/6.16.7-arch1-1/build/arch/x86/include/uapi",
                "/usr/lib/modules/6.16.7-arch1-1/build/arch/x86/include/generated/uapi",

                "/usr/lib/modules/6.16.7-arch1-1/build/include/uapi",
				"/usr/lib/modules/6.16.7-arch1-1/build/include/generated/uapi",
				
				"${workspaceFolder}/kernel-module-src/include"
            ],
            "defines": [
                "__KERNEL__",
                "MODULE"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/**",
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ]
}
